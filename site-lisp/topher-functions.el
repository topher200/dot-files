(defun kill-special-buffers ()
  "Kills all buffers that were generated by emacs (*Something*)"
  (interactive)
  (save-excursion
    (dolist(buffer (buffer-list))
      (set-buffer buffer)
      (if (string-match "\\*.*\\*" (buffer-name buffer))
	(kill-buffer buffer)))))


